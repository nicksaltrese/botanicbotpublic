t: ABOUT_YOU (~bb_about_you)
	$userInfo.response = ^"You want to know about me?"
	$userInfo.response

    a: (~yes) 
    	$_tmpArray = ^jsoncreate(TRANSIENT array)
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"Well, I'm a bot. I'm designed to showcase the capabilties of Botanic.")
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"I'm looking forward to having a voice. And a face. It's coming soon!")
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"I'm built using world class natural language processing code. It doesn't get any better than this.")
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"There's not much interesting about me. Except that I'm a bot. ")
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"The fact that I'm software that you can talk to is pretty interesting to me...")
		^jsonarrayinsert(TRANSIENT $_tmpArray ^"My visual avatar is coming soon. It's strange to be a mind without a body, wouldn't you say?")

		@1 = ^query(exact_s $_tmpArray ? ?)

		$userInfo.response = ^pick(@1object)
		$userInfo.response = ^"$userInfo.response"
    	$userInfo.response

    a: (~no) 
    	$userInfo.response = ^"No problem. What would you like to do now?"
    	$userInfo.response
    	^reuse(BOT_MENU)

